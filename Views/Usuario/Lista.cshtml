@using AxonAccessMVC.Models.Clases;
@{
    ViewBag.Title = "Axonia";

}
<!DOCTYPE html>
<html>

<body>

    @{ List<Usuario> usuarios = (List<Usuario>)ViewBag.usuarios;}
    <div class="row">
        <div class="col-md-12">
            <table class="table">
                <tr>
                    <th>Id_user</th>
                    <th>Role</th>
                    <th>Id_Estado</th>
                    <th>Id_Comuna</th>
                    <th>Id_Empresa</th>
                    <th>Rut</th>
                    <th>Dv</th>
                    <th>Nombre</th>
                    <th>App_Paterno</th>
                    <th>App_Materno</th>
                    <th>Direccion</th>
                    <th>Latitud</th>
                    <th>Longitud</th>
                    <th>Telefono</th>
                    <th>Mail</th>
                    <th>Password</th>
                </tr>

                @foreach (var usuario in usuarios)
                {

            <tr>
                <td>@usuario.Id</td>
                <td>@usuario.Desc_Role</td>
                <td>@usuario.Id_Estado</td>
                <td>@usuario.Id_Comuna</td>
                <td>@usuario.Id_Empresa</td>
                <td>@usuario.Rut</td>
                <td>@usuario.Dv</td>
                <td>@usuario.Nombre</td>
                <td>@usuario.App_Pater</td>
                <td>@usuario.App_Mater</td>
                <td>@usuario.Direccion</td>
                <td>@usuario.Latitud</td>
                <td>@usuario.Longitud</td>
                <td>@usuario.Telefono</td>
                <td>@usuario.Mail</td>
                <td>@usuario.Pass</td>
                <td>@Html.ActionLink("Eliminar", "Delete", new { id = usuario.Id })</td>
                <td>@Html.ActionLink("Modificar", "Edit", new { id = usuario.Id })</td>
            </tr>


                }
            </table>
        </div>

    </div>

</body>
</html>
