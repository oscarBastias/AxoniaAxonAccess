@using AxonAccessMVC.Models.Clases;
@model AxonAccessMVC.Models.Mae_Empresa

@{
    ViewBag.Title = "Axonia";
}

<!DOCTYPE html>
<html>
<head>
    <link href="~/styles/css/style.css" rel='stylesheet' type='text/css' media="all">
    <link href="//fonts.googleapis.com/css?family=Saira+Condensed:400,500,600,700" rel="stylesheet">
    <script type="text/javascript" src="js/jquery.js"></script>
    <script>
        function ddlselect() {
            var d = document.getElementById("ddselect").value;
            console.log(d);
        }
        ddlselect();
    </script>

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.css" />
    <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.js"></script>
</head>
<body>
    
    <form method="post">
        <div class="online-w3l-form">
            <div class="form-left-w3l">
                <select name="id_valor" id="ddselect" onchange="ddlselect();" class="form-control buttom" @Html.DropDownListFor(p => p.id_estamento, new SelectList(ViewBag.estamentos, "Id_Estamento", "desc_estamento")) </select>
            </div>
            <div class="btnn">
                <input type="submit" value="Lista">
            </div>
        </div>
        @{

            if (@TempData.Count > 0)
            {
                <script type="text/javascript">
                    var texto = ' @TempData["mensaje"]';
                    swal({
                        title: '',
                        text: texto,
                        type: 'success'
                    });

                </script>
            }
        }
    </form>

</body>
</html>

