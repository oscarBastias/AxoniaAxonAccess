@using AxonAccessMVC.Models.Clases;
@model AxonAccessMVC.Models.Clases.Mae_Empresa


@{
    ViewBag.Title = "Axonia";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <link href="~/styles/css/style.css" rel='stylesheet' type='text/css' media="all">
    <link href="//fonts.googleapis.com/css?family=Saira+Condensed:400,500,600,700" rel="stylesheet">

</head>
<body>

    <div class="online-w3l-form">

        <form action="#" method="post">
            @Html.HiddenFor(p => p.id_empresa)


            <div class="form-left-w3l">
                @Html.TextBoxFor(p => p.desc_empresa)
            </div>

            <div class="main">
                <div class="form-left-w3l">

                    <select class="form-control buttom" @Html.DropDownListFor(p => p.id_comuna, new SelectList(ViewBag.comunas, "Id_Comuna", "desc_comuna"))</select>

                </div>
                <div class="form-left-w3l">

                    <select class="form-control buttom" @Html.DropDownListFor(p => p.id_estamento, new SelectList(ViewBag.estamentos, "Id_Estamento", "desc_estamento"))</select>

                </div>
            </div>

            <div class="btnn">
                <input type="submit" value="Register">
            </div>
        </form>
    </div>

</body>
</html>
